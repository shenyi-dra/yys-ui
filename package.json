{
  "name": "yys-web-ui",
  "version": "0.0.1",
  "description": "web ui kit for yys",
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "src",
    "packages",
    "types"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "test": "cross-env NODE_ENV=development webpack --entry app=./src/main.js",
    "build": "cross-env NODE_ENV=development webpack --entry index=./packages/index.ts",
    "build:dev": "npm run build",
    "build:prod": "cross-env NODE_ENV=production webpack --entry index=./packages/index.ts",
    "server": "cross-env NODE_ENV=development webpack --entry app=./src/main.js && webpack-dev-server --color --progres --open",
    "server:dev": "npm run server",
    "start": "npm run build && npm run server",
    "start:dev": "npm run build && npm run server",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "element-ui": "^2.11.1",
    "vue": "^2.6.10",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.7.2",
    "@types/jasmine": "2.5.41",
    "@types/webpack": "^4.39.8",
    "@vue/babel-plugin-transform-vue-jsx": "^1.1.2",
    "@vue/babel-preset-jsx": "^1.1.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "element-theme-chalk": "^2.12.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.12.1",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.3.1"
  },
  "author": "sonnyshen",
  "license": "MIT"
}
